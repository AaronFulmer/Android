CREATE TABLE EMPLOYEE
(EMPLOYEE_ID DECIMAL(4) PRIMARY KEY,
 NAME_LAST CHAR(10),
 NAME_FIRST CHAR(10),
 EMAIL CHAR(30),
 PHONE_NUMBER CHAR(10),
 POSITION CHAR(10),
 USERNAME CHAR(10),
 PASSWORD CHAR(20));


